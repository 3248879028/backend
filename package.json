{
  "name": "corona-school-backend",
  "version": "0.3.0",
  "description": "The Corona School Backend",
  "engines": {
    "node": "13.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "echo Please be more specific what to start (for now...)",
    "build": "tsc",
    "build:clean": "rm -rf built && tsc",
    "test": "npm run build && jest",
    "test:debug": "npm run build && node --inspect --inspect-brk=9229 ./node_modules/.bin/jest --runInBand",
    "jobs": "node ./built/jobs/index.js",
    "jobs:dev": "node -r dotenv/config ./built/jobs/index.js",
    "jobs:debug": "npm run build && node --inspect --inspect-brk=9229 ./built/jobs/index.js",
    "web": "node ./built/web/index.js",
    "web:dev": "node -r dotenv/config ./built/web/index.js",
    "web:debug": "npm run build && node --inspect --inspect-brk=9229 ./built/web/index.js",
    "web:docs": "apidoc -i web/controllers -i common -o web/public/docs",
    "jest:clear": "./node_modules/.bin/jest --clearCache",
    "typeorm": "npm run build:clean && npx typeorm",
    "db:migration:create": "npm run typeorm -- migration:create -n",
    "db:migration:generate": "npm run typeorm -- migration:generate -n",
    "db:migration:run": "npm run typeorm -- migration:run",
    "db:migration:revert": "npm run typeorm -- migration:revert",
    "db:migration:show": "npm run typeorm -- migration:show"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.4",
    "@types/html-pdf": "^2.2.0",
    "@types/node": "^12.12.31",
    "@types/node-mailjet": "^3.3.3",
    "@types/papaparse": "^5.0.3",
    "@types/react-select": "^3.0.12",
    "@types/textversionjs": "^1.1.0",
    "async-mutex": "^0.2.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cron": "^1.8.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.5.6",
    "escape-html": "^1.0.3",
    "esm": "^3.2.25",
    "express": "^4.15.2",
    "express-favicon": "^2.0.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-pdf": "^2.2.0",
    "log4js": "latest",
    "moment": "^2.24.0",
    "node-mailjet": "^3.3.1",
    "nodemailer": "^6.4.5",
    "papaparse": "^5.1.1",
    "paseto.js": "^0.1.6",
    "pg": "^7.18.2",
    "react-select": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "textversionjs": "^1.1.3",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/rewire": "^2.5.28",
    "apidoc": "^0.22.1",
    "cors": "^2.8.5",
    "jest": "^25.3.0",
    "libsodium-wrappers": "^0.7.6",
    "request": "^2.81.0",
    "rewire": "^5.0.0",
    "tape": "^4.7.0",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geroembser/corona-school"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
